<?xml version="1.0" encoding="UTF-8"?>
<metamorph xmlns="http://www.culturegraph.org/metamorph" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	version="1">
	<rules>	<!-- NRW reverse geocoding -->
		<combine name="url"
			value="http://api.geonames.org/findNearbyPlaceNameJSON?lat=${lat}&amp;lng=${lon}&amp;username=dr0i&amp;format=json&amp;style=SHORT&amp;callback=${id}">
			<data source="lat" name="lat"/>
			<data source="lon" name="lon"/>
			<data source="json_callback" name="id">
				<replace pattern="\(" with=""/>
			</data>
		</combine>
	</rules>
</metamorph>