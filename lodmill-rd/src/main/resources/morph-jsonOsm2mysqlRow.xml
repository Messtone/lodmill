<?xml version="1.0" encoding="UTF-8"?>
<metamorph xmlns="http://www.culturegraph.org/metamorph" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	version="1">
	<rules>	<!-- NRW geoLocation -->
		<!-- store without prefix 'http://www.openstreetmap.org/' -->
		<combine name="${id}" value="${osm_type}/${osm_id}">
			<data source="osm_type" name="osm_type"/>
			<data source="osm_id" name="osm_id"/>
			<data source="json_callback" name="id">
				<replace pattern="\(" with=""/>
			</data>
		</combine>
	</rules>
</metamorph>